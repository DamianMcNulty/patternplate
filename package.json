{
  "name": "patternplate",
  "version": "0.8.0",
  "description": "Create flexible, powerful and comprehensive Living Styleguides with ease.",
  "main": "library/",
  "scripts": {
    "start": "node --harmony binary/patternplate.js",
    "console": "node --harmony binary/patternplate-console",
    "build": "npm run babel",
    "watch": "npm run babel -- --watch",
    "babel": "npm run clean && babel source --out-dir ./",
    "clean": "rm -rf binary library application configuration"
  },
  "bin": {
    "patternplate": "binary/patternplate.js",
    "patternplate-console": "binary/patternplate-console.js"
  },
  "dependencies": {
    "babel-core": "^5.2.17",
    "bluebird": "^2.9.25",
    "boilerplate-server": "git+ssh://git@git.sinnerschrader.com:patternplate/boilerplate-server.git#v0.6.1",
    "lru-cache": "^2.6.2",
    "minimist": "^1.1.1",
    "normalize.less": "^1.0.0",
    "patternplate-client": "git+ssh://git@git.sinnerschrader.com:patternplate/patternplate-client.git#v0.6.1",
    "patternplate-server": "git+ssh://git@git.sinnerschrader.com:patternplate/patternplate-server.git#v0.8.4",
    "resolve-path": "^1.2.2"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "babel-eslint": "^3.0.1",
    "eslint": "^0.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sinnerschrader.com:patternplate/patternplate.git"
  },
  "author": {
    "name": "SinnerSchrader - Team UX",
    "email": "team-ux@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "iojs": ">= 1.4.3",
    "node": ">= 0.12.0",
    "npm": ">= 2.5.1"
  }
}
