{
  "name": "patternplate",
  "version": "0.3.0",
  "description": "Create flexible, powerful and comprehensive Living Styleguides with ease.",
  "main": "library/",
  "scripts": {
    "start": "node --harmony binary/patternplate.js",
    "build": "npm run babel",
    "watch": "npm run babel -- --watch",
    "babel": "npm run clean && babel source --out-dir ./",
    "clean": "rm -rf binary library"
  },
  "bin": {
    "patternplate": "binary/patternplate.js"
  },
  "dependencies": {
    "babel-core": "^5.2.17",
    "boilerplate-server": "git+ssh://git@git.sinnerschrader.com:patternplate/boilerplate-server.git",
    "minimist": "^1.1.1",
    "patternplate-client": "git+ssh://git@git.sinnerschrader.com:patternplate/patternplate-client.git",
    "patternplate-server": "git+ssh://git@git.sinnerschrader.com:patternplate/patternplate-server.git"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "babel-eslint": "^3.0.1",
    "eslint": "^0.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sinnerschrader.com:patternplate/patternplate.git"
  },
  "author": {
    "name": "SinnerSchrader - Team UX",
    "email": "team-ux@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
