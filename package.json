    "lru-cache": "^2.6.2",
    "minimist": "^1.1.1",
    "normalize.less": "^1.0.0",
    "patternplate-client": "^0.11.7",
    "patternplate-server": "^0.12.2",
    "resolve-path": "^1.2.2"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "babel-eslint": "^3.0.1",
    "eslint": "^0.20.0"
  },
